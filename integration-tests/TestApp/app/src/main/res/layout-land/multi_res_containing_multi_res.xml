<layout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:bind="http://schemas.android.com/apk/res-auto">
    <data>
        <variable name="outerObservableInt" type="androidx.databinding.ObservableInt"/>
        <variable name="outerPlainInt" type="int"/>
        <variable name="outerBoxedInt" type="java.lang.Integer"/>
    </data>
    <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

        <include layout="@layout/multi_res_layout" android:id="@+id/includedMultiResLayout"
                 bind:innerObservableInt="@{outerObservableInt}"
                 bind:innerPlainInt1="@{outerObservableInt}"
                 bind:innerPlainInt2="@{outerPlainInt}"
                 bind:innerPlainInt3="@{outerBoxedInt}"
                 bind:innerBoxedInt1="@{outerObservableInt}"
                 bind:innerBoxedInt2="@{outerPlainInt}"
                 bind:innerBoxedInt3="@{outerBoxedInt}"
                />
    </LinearLayout>
</layout>